{
  "type": "commonjs",
  "name": "h5",
  "version": "1.0.0",
  "author": "bruce",
  "browserslist": {
    "development": [
      "last 1 chrome version"
    ],
    "production": [
      "iOS >= 8",
      "Android >= 4"
    ]
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "build:dev": "yarn build --mode dev",
    "build:test": "yarn build --mode test",
    "build:sit1": "yarn build --mode sit1",
    "build:uat": "yarn build --mode uat",
    "build:sit2": "yarn build --mode sit2",
    "build:sit3": "yarn build --mode sit3",
    "build:prod": "yarn build --mode production",
    "serve": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git add . && git-cz",
    "deps": "yarn upgrade-interactive --latest",
    "deleteTiny": "node ./nodeScript/deleteTiny.js",
    "tinyCurImg": "node ./nodeScript/tinyCurImg.js"
  },
  "dependencies": {
    "@vueuse/components": "^10.2.0",
    "@vueuse/core": "^10.2.0",
    "axios": "^1.4.0",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.8",
    "decimal.js": "^10.4.3",
    "fingerprintjs2": "^2.1.4",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "platform": "^1.3.6",
    "qs": "^6.11.1",
    "vant": "^4.7.0",
    "vconsole": "^3.15.1",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2",
    "weixin-js-sdk": "^1.6.0",
    "xgplayer": "3.0.17",
    "xgplayer-hls": "3.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/node": "^20.7.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "axios-mapper": "^0.5.9",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.7.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "1.21.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.3",
    "import": "^0.0.6",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "13.2.2",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "plop": "^3.1.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.8.0",
    "stylelint-config-recommended": "^12.0.0",
